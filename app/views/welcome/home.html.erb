
<h1>Welcome Saigon Resto</h1>

<video width="100%" height="35%" autoplay>
  <source src="/videos/avo-egg.mp4" type="video/mp4"/>
</video>


<ul class="nolist">
  <% @items.each do |i| %>
    <li><%= i.name %></li>
    <li><%= i.description %></li>
    <li><%= image_tag i.image unless i.image.nil? %></li>
    <li><%= i.price %></li>
  <% end %>
</ul>


